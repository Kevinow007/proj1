//Display
public String displayValue() {
String value1 = setvalue1("test1");
String value2 = setvalue2("test2");
String value3 = setvalue2("test3");
return value1 + " " + value2 + " " + value3;
}

// Function setvalue1
private String val1 = "";
public void setvalue1(String s1) {
val1 = s1;
}

// Function setvalue2
private String val2 = "";
public void setvalue2(String s2) {
val2 = s2;
}

// Function setvalue3
private String val3 = "";
public void setvalue3(String s3) {
val3 = s3;
}

// cacat chaine caractere et retourner la chaine
public String cacat() {
String s = setvalue1("a") + setvalue2("b") + setvalue3("c");
return s;
}
